VERSION 1. BLINK test function. 

  Turns an LED on for one second, then off for one second, repeatedly. Altered delay 3000 ms for more visible 3 seconds. 
 
  modified 8 May 2014
  by Scott Fitzgerald
  modified 2 Sep 2016
  by Arturo Guadalupi
  modified 8 Sep 2016
  by Colby Newman
  Modified by Miikkä Mäki and Tom Railio 20062022. 

  This example code is in the public domain.

  https://www.arduino.cc/en/Tutorial/BuiltInExamples/Blink


// the setup function runs once when you press reset or power the board
void setup() {
  // initialize digital pin LED_BUILTIN as an output.
  pinMode(LED_BUILTIN, OUTPUT);
}

// the loop function runs over and over again forever
void loop() {
  digitalWrite(LED_BUILTIN, HIGH);   // turn the LED on (HIGH is the voltage level)
  delay(1000);                       // wait for a second
  digitalWrite(LED_BUILTIN, LOW);    // turn the LED off by making the voltage LOW
  delay(3000);                       // wait for three seconds. 
}
  

Version 2. BASIC initializations. 


// Protopaja 2022
// Group 4: SAAB Finland
// Handheld Microwave Transmitter; "The Death Star"

// Set up antenna controlling pin
  int antennaPin = 25;
// Set up pins to read if button is pressed
  int buttonIn =  32;
  int buttonOut = 35;
  
void setup() {
  // Sets antennPin and buttonIn to send out signals
  pinMode(antennaPin, OUTPUT);
  pinMode(buttonIn, OUTPUT);
  // Sets buttonOUt to receive signals
  pinMode(buttonOut, INPUT);
}

void loop() {
  // Power on the button
  digitalWrite(buttonIn, HIGH);
  
  // If button is pressed, send turn on signal to the antenna pin
  if (digitalRead(buttonOut)== 1) {
    digitalWrite(antennaPin, HIGH);
  }
  // If button is not pressed, send turn off signal to antenna  pin
  else digitalWrite(antennaPin, LOW);

}


Version 3. BASIC script. 

// Protopaja 2022
// Group 4: SAAB Finland
// Handheld Microwave Transmitter; "The Death Star"

#include <WiFi.h>

///////please enter your sensitive data in the Secret tab/arduino_secrets.h
char ssid[] = "aalto open";        // your network SSID (name)
char pass[] = "";        // your network password (use for WPA, or use as key for WEP)
int activePin = 25;
int buttonState = 0;

void setup() {
  
  pinMode(activePin, OUTPUT);
  Serial.begin(9600);             //Initialize serial and wait for port to open:

}

void loop() {
                               // check the network connection once every 15 seconds:
 digitalWrite(activePin, HIGH);
 buttonState = digitalRead(activePin);
 if (buttonState == 1) {
  WiFi.begin(ssid, pass);
 }
}




